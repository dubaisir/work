{
    "job": { 
        "setting": {
            "speed": {
                "channel": 5
            },
                "errorLimit":{
                "record":0,
                "percentage":0
            }
        },
        "content": [
            {
                "reader": {
                    "name": "mysqlreader",
                    "parameter": {
                        "username": "${SRC_MYSQL_RD_USR}",
                        "password": "${SRC_MYSQL_RD_PWD}",
                        "connection": [
                            {
                                "querySql": [
                                    "select  
                                         projectname, 
                                         quotaname, 
                                         january, 
                                         february, 
                                         march, 
                                         april, 
                                         may, 
                                         june, 
                                         july, 
                                         august, 
                                         september, 
                                         october, 
                                         november, 
                                         december, 
                                         create_time, 
                                         update_time, 
                                         '${CURRENT_FLOW_START_DAY}',
                                         ${EXECUTION_ID} 
                                    from raqsoft.smartbi_business"
                                ],
                                "jdbcUrl": [
                                    "${SRC_MYSQL_RD_URL}"
                                ]
                            }
                        ]
                    }
                },
                "writer": {
                    "name": "hdfswriter",                              
                    "parameter": { 
                        "column": [
                            {
                                "name": "projectname",
                                "type": "string"
                            },

                            {
                                "name": "quotaname",
                                "type": "string"
                            },

                            {
                                "name": "january",
                                "type": "double"
                            },

                            {
                                "name": "february",
                                "type": "double"
                            },

                            {
                                "name": "march",
                                "type": "double"
                            },

                            {
                                "name": "april",
                                "type": "double"
                            },

                            {
                                "name": "may",
                                "type": "double"
                            },

                            {
                                "name": "june",
                                "type": "double"
                            },

                            {
                                "name": "july",
                                "type": "double"
                            },

                            {
                                "name": "august",
                                "type": "double"
                            },

                            {
                                "name": "september",
                                "type": "double"
                            },

                            {
                                "name": "october",
                                "type": "double"
                            },

                            {
                                "name": "november",
                                "type": "double"
                            },

                            {
                                "name": "december",
                                "type": "double"
                            },

                            {
                                "name": "create_time",
                                "type": "string"
                            },

                            {
                                "name": "update_time",
                                "type": "string"
                            },
                            {
                                "name": "op_time",
                                "type": "string"
                            },
                            {
                                "name": "execution_id",
                                "type": "string"
                            }
                        ],
                        "defaultFS": "${HDFS_ROOT_URI}",
                        "fieldDelimiter": "\t",
                        "fileName": "smartbi_business",
                        "fileType": "orc",
                        "path": "${HDFS_HV_EXTTB_ROOT}/${HIVE_DB_ODS_DIM}/smartbi_business/load_date=${CURRENT_FLOW_START_DAY}",
                        "writeMode": "append"
                    },
                    "extendScript": {
                        "post": [
                            {
                                "type":"hql",
                                "script":"ALTER TABLE ${HIVE_DB_ODS_DIM}.smartbi_business ADD IF NOT EXISTS PARTITION(load_date='${CURRENT_FLOW_START_DAY}')"
                            }
                        ]
                    }
                }
            }
        ]
    }
}