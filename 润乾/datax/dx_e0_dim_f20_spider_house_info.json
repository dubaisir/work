{
    "job": { 
        "setting": {
            "speed": {
                "channel": 5
            },
                "errorLimit":{
                "record":0,
                "percentage":0
            }
        },
        "content": [
            {
                "reader": {
                    "name": "mysqlreader",
                    "parameter": {
                        "username": "${SRC_MYSQL_RD_USR}",
                        "password": "${SRC_MYSQL_RD_PWD}",
                        "connection": [
                            {
                                "querySql": [
                                    "select  
                                         room_id, 
                                         village_id, 
                                         village, 
                                         city, 
                                         location_region, 
                                         location_center, 
                                         min_area, 
                                         max_area, 
                                         towards, 
                                         model, 
                                         lon, 
                                         lat, 
                                         create_time, 
                                         update_time, 
                                         '${CURRENT_FLOW_START_DAY}',
                                         ${EXECUTION_ID} 
                                    from raqsoft.f20_spider_house_info
                                    where date(update_time) >= '${LAST_EXTRACT_DAY}'"
                                ],
                                "jdbcUrl": [
                                    "${SRC_MYSQL_RD_URL}"
                                ]
                            }
                        ]
                    }
                },
                "writer": {
                    "name": "hdfswriter",                              
                    "parameter": { 
                        "column": [
                            {
                                "name": "room_id",
                                "type": "string"
                            },

                            {
                                "name": "village_id",
                                "type": "string"
                            },

                            {
                                "name": "village",
                                "type": "string"
                            },

                            {
                                "name": "city",
                                "type": "string"
                            },

                            {
                                "name": "location_region",
                                "type": "string"
                            },

                            {
                                "name": "location_center",
                                "type": "string"
                            },

                            {
                                "name": "min_area",
                                "type": "double"
                            },

                            {
                                "name": "max_area",
                                "type": "double"
                            },

                            {
                                "name": "towards",
                                "type": "string"
                            },

                            {
                                "name": "model",
                                "type": "string"
                            },

                            {
                                "name": "lon",
                                "type": "string"
                            },

                            {
                                "name": "lat",
                                "type": "string"
                            },

                            {
                                "name": "create_time",
                                "type": "string"
                            },

                            {
                                "name": "update_time",
                                "type": "string"
                            },
                            {
                                "name": "op_time",
                                "type": "string"
                            },
                            {
                                "name": "execution_id",
                                "type": "string"
                            }
                        ],
                        "defaultFS": "${HDFS_ROOT_URI}",
                        "fieldDelimiter": "\t",
                        "fileName": "f20_spider_house_info",
                        "fileType": "orc",
                        "path": "${HDFS_HV_EXTTB_ROOT}/${HIVE_DB_ODS_DIM}/f20_spider_house_info/load_date=${CURRENT_FLOW_START_DAY}",
                        "writeMode": "append"
                    },
                    "extendScript": {
                        "post": [
                            {
                                "type":"hql",
                                "script":"ALTER TABLE ${HIVE_DB_ODS_DIM}.f20_spider_house_info ADD IF NOT EXISTS PARTITION(load_date='${CURRENT_FLOW_START_DAY}')"
                            }
                        ]
                    }
                }
            }
        ]
    }
}